{
	"editor.defaultFormatter": "biomejs.biome",
	"editor.codeActionsOnSave": {
		"quickfix.biome": "explicit"
	},
	"editor.formatOnSave": true,
	"[typescriptreact]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[typescript]": {
		"editor.defaultFormatter": "vscode.typescript-language-features"
	},
	"[javascript]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"biome.enable": true,
	"biome.requireConfig": true,
	"files.associations": {
		"*.tsx": "typescriptreact"
	}
}